{
  "builds": [
    {
      "src": "darf_vermieter_das/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "staticfiles/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "darf_vermieter_das/wsgi.py"
    }
  ]
}